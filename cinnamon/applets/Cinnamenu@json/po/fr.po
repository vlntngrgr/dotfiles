#. metadata.json->name
#. settings-schema.json->cinnamenuProvidersSection->title
msgid "Cinnamenu"
msgstr "Cinnamenu"

#. metadata.json->description
msgid "A flexible menu providing formatting options, web bookmarks, open window lookup, and search provider support with fuzzy searching."
msgstr "Un menu flexible offrant des options de formatage, des signets Web, une recherche parmi vos fichiers et applications, et un support de moteurs de recherche sur le web."

#. settings-schema.json->panel->title
msgid "Behavior"
msgstr "Comportement"

#. settings-schema.json->appearance->title
msgid "Appearance"
msgstr "Apparence"

#. settings-schema.json->menu->title
msgid "Layout and Content"
msgstr "Mise en page et contenu"

#. settings-schema.json->providers->title
msgid "Search Providers"
msgstr "Moteurs de Recherche"

#. settings-schema.json->extensionProvidersSection->title
msgid "No search provider extensions found"
msgstr "Aucune extension pour moteur de recherche n'a été installée"

#. settings-schema.json->panel-appear->title
#. settings-schema.json->panel-behave->title
